{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=@myapp/web --filter=@myapp/mobile --filter=@myapp/desktop",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run type-check"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "sql.js": "^1.14.0"
  }
}
